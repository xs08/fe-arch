{
  "extends": "./src/.umi/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@@/*": ["src/.umi/*"],
      "umi/typings": ["src/.umi/typings"],
      "@Apis/*": ["src/api/*"],
      "@Assets/*": ["src/assets/*"],
      "@Components/*": ["src/components/*"],
      "@Events/*": ["src/events/*"],
      "@Hooks/*": ["src/hooks/*"],
      "@Layouts/*": ["src/layouts/*"],
      "@Styles/*": ["src/styles/*"],
      "@Tracking/*": ["src/tracking/*"],
      "@Utils/*": ["src/utils/*"],
      "@Redux/*": ["src/redux/*"],
      "@Actions/*": ["src/redux/actions/*"],
      "@DataModel/*": ["src/redux/dataModel/*"],
      "@DataHooks/*": ["src/redux/hooks/*"]
    }
  },
  "include": [
    "typings.d.ts",
    "node_modules/umi",
    "config/*.ts",
    "mock/*.ts",
    "**/src/**/*.ts",
    "**/src/**/*.tsx"
  ]
}
