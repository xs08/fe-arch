{
  "printWidth": 110,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "proseWrap": "never",
  "overrides": [{
    "files": ".prettierrc",
    "options": { "parser": "json" }
  }],
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-packagejson"]
}
